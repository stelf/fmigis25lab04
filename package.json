{
  "name": "postgis2web",
  "version": "1.0.0",
  "description": "Web application to visualize PostGIS data on a map.",
  "main": "postgis2web.js",
  "scripts": {
    "start-duckdb": "node srv/duckdb2web.js",
    "start-postgis": "node srv/postgis2web.js",
    "start": "npm-run-all --parallel start-duckdb start-postgis",
    "postinstall": "npm install npm-run-all"
  },
  "keywords": [
    "PostGIS",
    "DuckDB",
    "Leaflet",
    "Node.js",
    "Express",
    "Geoapify"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@duckdb/node-api": "^1.3.0-alpha.21",
    "dotenv": "^16.5.0",
    "duckdb": "^1.2.1",
    "express": "^4.21.2",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.7",
    "pg": "^8.7.3"
  }
}
